{
    "name": "tab-and-bookmark",
    "displayName": "tab-and-bookmark",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.94.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "tab-and-bookmark",
                    "title": "Package Explorer",
                    "icon": "images/my-icon.svg"
                }
            ]
        },
        "views": {
            "tab-and-bookmark": [
                {
                    "id": "tabView",
                    "name": "tab",
                    "type": "tree",
                    "contextualTitle": "tab"
                },
                {
                    "id": "bookmartView",
                    "name": "bookmarks",
                    "type": "tree"
                }
            ]
        },
        "commands": [],
        "menus": {
            "view/title": [],
            "view/item/context": [],
            "editor/title/context": [],
            "explorer/context": []
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.2.2",
        "@types/mocha": "^10.0.8",
        "@types/node": "20.x",
        "@types/vscode": "^1.94.0",
        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.11.1",
        "typescript": "^5.6.2"
    },
    "dependencies": {
        "crypto-js": "^4.2.0",
        "nanoid": "^5.0.9",
        "uuid": "^11.0.3"
    }
}
