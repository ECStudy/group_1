{
    "name": "tab-and-bookmark",
    "displayName": "tab-and-bookmark",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.94.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "tab-and-bookmark",
                    "title": "Package Explorer",
                    "icon": "images/my-icon.svg"
                }
            ]
        },
        "views": {
            "tab-and-bookmark": [
                {
                    "id": "tabview",
                    "name": "tab",
                    "type": "tree",
                    "contextualTitle": "tab"
                },
                {
                    "id": "bookmartView",
                    "name": "bookmarks",
                    "type": "tree"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "tab-and-bookmark.tabview.create.group",
                    "when": "view == tabview",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "tab-and-bookmark.tabview.close.tab",
                    "when": "viewItem == tab",
                    "group": "inline"
                },
                {
                    "command": "tab-and-bookmark.tabview.update.group",
                    "when": "viewItem == group",
                    "group": "inline"
                },
                {
                    "command": "tab-and-bookmark.tabview.delete.group",
                    "when": "viewItem == group",
                    "group": "inline"
                },
                {
                    "command": "tab-and-bookmark.tabview.open.group",
                    "when": "viewItem == group",
                    "group": "inline"
                },
                {
                    "command": "tab-and-bookmark.tabview.create.TabToGroupContext",
                    "when": "viewItem == tab",
                    "group": "navigation"
                },
                {
                    "command": "tab-and-bookmark.tabview.create.TabToNewGroup",
                    "when": "viewItem == tab",
                    "group": "navigation"
                }
            ],
            "editor/title/context": [
                {
                    "command": "tab-and-bookmark.tabview.create.TabToGroup",
                    "group": "navigation"
                },
                {
                    "command": "tab-and-bookmark.tabview.create.TabToNewGroup",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "tab-and-bookmark.tabview.create.TabToGroup",
                    "group": "navigation"
                },
                {
                    "command": "tab-and-bookmark.tabview.create.TabToNewGroup",
                    "group": "navigation"
                }
            ]
        },
        "commands": [
            {
                "command": "tab-and-bookmark.tabview.close.tab",
                "title": "Close Tab",
                "icon": "$(close)"
            },
            {
                "command": "tab-and-bookmark.tabview.update.group",
                "title": "Edit Group",
                "icon": "$(edit)"
            },
            {
                "command": "tab-and-bookmark.tabview.delete.group",
                "title": "Close Group",
                "icon": "$(close)"
            },
            {
                "command": "tab-and-bookmark.tabview.open.group",
                "title": "Open Group",
                "icon": "$(eye)"
            },
            {
                "command": "tab-and-bookmark.tabview.create.group",
                "title": "Create Group",
                "icon": "$(add)"
            },
            {
                "command": "tab-and-bookmark.tabview.create.TabToGroup",
                "title": "기존 그룹에 탭 추가",
                "category": "tab"
            },
            {
                "command": "tab-and-bookmark.tabview.create.TabToGroupContext",
                "title": "기존 그룹에 탭 추가",
                "category": "tab"
            },
            {
                "command": "tab-and-bookmark.tabview.create.TabToNewGroup",
                "title": "새 그룹에 추가",
                "category": "tab"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.2.2",
        "@types/mocha": "^10.0.8",
        "@types/node": "20.x",
        "@types/vscode": "^1.94.0",
        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.11.1",
        "typescript": "^5.6.2"
    },
    "dependencies": {
        "crypto-js": "^4.2.0",
        "nanoid": "^5.0.9",
        "uuid": "^11.0.3"
    }
}
