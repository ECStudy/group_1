{
    "name": "vscode-tabgroup-and-bookmark",
    "displayName": "grop tab",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.94.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "vscode-tabgroup-and-bookmark",
                    "title": "Package Explorer",
                    "icon": "images/my-icon.svg"
                }
            ]
        },
        "views": {
            "vscode-tabgroup-and-bookmark": [
                {
                    "id": "TabsTreeView",
                    "name": "Tabs",
                    "type": "tree",
                    "contextualTitle": "Tabs"
                },
                {
                    "id": "bookmark-view",
                    "name": "Bookmarks",
                    "type": "tree"
                }
            ]
        },
        "commands": [
            {
                "command": "tabView.refresh",
                "title": "Refresh Open Files"
            },
            {
                "command": "tabView.createGroup",
                "title": "Create Group",
                "icon": "$(add)"
            },
            {
                "command": "tabView.deleteGroup",
                "title": "Delete Group",
                "icon": "$(remove)"
            },
            {
                "command": "tabView.openAll",
                "title": "Delete Group",
                "icon": "$(eye)"
            },
            {
                "command": "tabView.addToGroup",
                "title": "추가추가추가추가",
                "category": "Tabs"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "tabView.createGroup",
                    "when": "view == TabsTreeView",
                    "group": "navigation"
                },
                {
                    "command": "tabView.deleteGroup",
                    "when": "view == TabsTreeView",
                    "group": "navigation"
                },
                {
                    "command": "tabView.openAll",
                    "when": "view == TabsTreeView",
                    "group": "navigation"
                }
            ],
            "editor/title/context": [
                {
                    "command": "tabView.addToGroup",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "tabView.addToGroup",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.94.0",
        "@types/mocha": "^10.0.8",
        "@types/node": "20.x",
        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "eslint": "^9.11.1",
        "typescript": "^5.6.2",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1"
    }
}
